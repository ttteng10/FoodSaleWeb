//회원 테이블 만들기
create table member(
    memberID varchar(50),
    password varchar(50) not null,
    name varchar(50),
    email varchar(50),
    address varchar(50),
    gender varchar(10),
    birth varchar(20),
    primary key(memberId)
);
//장바구니 테이블 만들기
create table basketEx(
    memberID varchar(50),
    itemNum varchar(100),
    quantity integer
);
//좋아요 테이블 만들기
create table likeboxEx(
    memberID varchar(50),
    itemNum varchar(50)
);
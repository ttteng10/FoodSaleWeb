//상품테이블만들기
create table item (
    itemNum varchar(50),
    itemCode varchar(50),
    itemName varchar(50),
    itemSalePercent varchar(50),
    itemSale varchar(50),
    itemPrice varchar(50),
    saleUnit varchar(50),
    weight varchar(50),
    imgSrc varchar(50),
    primary key(itemNum)
)
